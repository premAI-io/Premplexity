{
  "name": "premplexity-webapp",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "scripts": {
    "server:dev": "nodemon",
    "dev": "./dev.sh",
    "test": "",
    "makemigrations": "drizzle-kit generate",
    "dropmigration": "drizzle-kit drop",
    "custommigration": "drizzle-kit generate --custom",
    "migrate": "node --import tsx -r dotenv/config -r reflect-metadata src/scripts/migrate",
    "xss:scan": "xss-scan",
    "start": "node -r dotenv/config -r reflect-metadata dist/index.js",
    "build": "tsx -r dotenv/config build.ts",
    "db:seed": "node --import tsx -r dotenv/config -r reflect-metadata src/scripts/seed",
    "db:drop": "node --import tsx -r dotenv/config -r reflect-metadata src/scripts/drop",
    "db:truncate": "node --import tsx -r dotenv/config -r reflect-metadata src/scripts/truncate",
    "db:reset": "npm run db:drop && npm run migrate && npm run db:seed",
    "db:reseed": "npm run db:truncate && npm run db:seed",
    "generate:password": "node --import tsx -r dotenv/config -r reflect-metadata src/scripts/generate-password",
    "generate:templates-enum": "node --import tsx -r dotenv/config -r reflect-metadata src/scripts/generate-mail-templates-enum"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/premAI-io/Premplexity"
  },
